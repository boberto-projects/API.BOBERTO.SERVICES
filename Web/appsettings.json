{
  "ConnectionStrings": {
    "RedisConnectionContext": "%REDIS_URL%"
  },
  "ApiConfig": {
    "Swagger": true,
    "Authorization": {
      "Activate": true,
      "ApiHeader": "ApiKey",
      "ApiKey": "%API_KEY%"
    }
  },
  "DiscordApiConfig": {
    "Enabled": true,
    "WebHooks": [
      {
        "Channel": "geral",
        "Url": "%DISCORD_GERAL_URL%"
      }
    ]
  },
  "SmtpConfig": {
    "Host": "%SMTP_HOST%",
    "From": "%SMTP_FROM%",
    "Port": "%SMTP_PORT%",
    "Username": "%SMTP_USERNAME%",
    "Password": "%SMTP_PASSWORD%"
  },
  "ZenvioSecurityConfig": {
    "SecondsExpiration": 86400,
    "MaximumAttempts": 3
  },
  "NtfyApiConfig": {
    "Url": "https://ntfy.sh"
  },
  "ZenviaApiConfig": {
    "Enabled": true,
    "ApiKey": "%ZENVIA_APIKEY%",
    "Alias": "%ZENVIA_ALIAS%",
    "Url": "https://api.zenvia.com/v2"
  },
  "SmsAdbTesterApiConfig": {
    "Enabled": true,
    "Url": "http://host.docker.internal:6666"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}